
/* view 默认样去除*/
.copy_code_box {
  display: flex;
  align-items: center;
  flex: 1;
  width: auto;
  background: #0d1117;
  position: relative;
  height: 24px;
  border-top-left-radius: 6px;
  border-top-right-radius: 6px;
  margin: 0 4rpx;
}

.copy_code {
  display: flex;
  align-items: center;
  color:forestgreen;
  position: absolute;
  right: 4px;
  padding: 4px 8px;
  border-radius: 20px;
  font-size: 26rpx;
}
